{
	"language": "en-GB",
	"ignorePaths": [
		"**/node_modules/**",
		"LICENSE",
		"**/coverage/**",
		"**/dist/**",
		"**/docs/reference/**",
		"**/docs/open-api/**",
		"**/tests/**",
		"*.yaml",
		"package.json",
		"rollup.config.mjs",
		"vitest.config.ts",
		"tsconfig.json",
		"typedoc.json",
		".eslintrc.cjs",
		"**/wordlists/**"
	],
	"words": [
		"GTSC",
		"nameof",
		"openapi",
		"Programatically",
		"Stiftung",
		"tspc",
		"ttsc",
		"ttypescript"
	],
	"ignoreRegExpList": ["/from\\s+(['\"]).*\\1/", "/eslint-disable.*/"],
	"ignoreWords": ["Cunknown"]
}
